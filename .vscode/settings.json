{
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.tabSize": 4,
  "[json]": {
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "python.languageServer": "None",
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    }
  },
  "ruff.path": [
    "/workspaces/learn-gcp-cloud-run/app/.venv/bin/ruff"
  ],
  "ruff.configuration": "/workspaces/learn-gcp-cloud-run/app/ruff.toml",
  "ty.path": [
    "/workspaces/learn-gcp-cloud-run/app/.venv/bin/ty"
  ],
  "ty.configurationFile": "/workspaces/learn-gcp-cloud-run/app/ty.toml",
  "cSpell.words": [
    "charliermarsh",
    "codezombiech",
    "commandhistory",
    "debugpy",
    "dhoeric",
    "eamodio",
    "isort",
    "mhutchie",
    "njpwerner",
    "pycache",
    "pycodestyle",
    "pydocstyle",
    "Pyflakes",
    "pyproject",
    "pytest",
    "pyupgrade",
    "shellautocompletion",
    "tamasfe",
    "trixie",
    "tyriar",
    "venv",
    "wayou",
    "yzhang"
  ]
}